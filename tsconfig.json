{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "rootDir": "./src",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
